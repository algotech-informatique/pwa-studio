    "homepage": "https://www.algotech.vision",
    "scripts": {
        "ng": "ng",
        "start": "export NODE_OPTIONS=--max_old_space_size=8192 && ionic serve",
        "ng:serve": "ng serve",
        "serve-prod": "http-server ./www -c-1",
        "build": "export NODE_OPTIONS=--max_old_space_size=8192 && ng build --base-href /studio/",
        "build:prod": "export NODE_OPTIONS=--max_old_space_size=8192 && ng build --configuration production",
        "test": "ng test",
        "test:cov": "ng test --code-coverage=true --code-coverage-exclude='*.component.ts,*.schema.ts'",
        "test:unit": "jest --ci",
        "test:unit:watch": "nodemon --watch src/app/ --ext ts --exec 'npm run test:unit'",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "sonar": "npm run check-dep && sonar-scanner"
    },
